init() {
	JAVA_MIN_MEM=16m
	JAVA_MAX_MEM=32m
}

run() {

    exec $JAVA $JAVA_OPTS $OPTS -classpath "$CLASSPATH" org.fusesource.cloudlaunch.launcher.Main -dataDir $CL_HOME/data $*
}

main() {
    init
    . setenv
    run $@
}

main $@
